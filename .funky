{"new-post": "earthly --build-arg name=\"$1\" +new-post", "lint": "earthly +blog-lint-apply \"$@\"", "post": "echo \"blog/_posts/$(ls -Art blog/_posts | tail -n 1)\"", "image-folder": "echo \"$(pwd)/$(ls -Artd blog/assets/images/* | grep -v \"authors\" | tail -n 1)\"", "set-header": "cp \"$(latest-image)\" \"$(image-folder)/header.jpg\"", "set-image": "cp \"$(latest-image)\" \"$(image-folder)/$(date +%s).png\"", "gen-image-folder": "echo \"$(pwd)/blog/_site/generated/assets/images/$(ls -Art blog/_site/generated/assets/images/ | tail -n 1)\"", "clear-images": "rm -f \"$(gen-image-folder)/*\"", "set-author-image": "cp \"$(latest-image)\" \"$(pwd)/blog/assets/images/authors/$1.jpg\"", "link-opp": "earthly +link-opportunity --NAME=\"$(ls -Art blog/_posts | tail -n 1)\""}